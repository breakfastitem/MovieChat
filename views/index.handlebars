<!--Navigation bar-->
<h1>Movie Chat</h1>

<div class="container">
    <div class="row">
        <div class="col-12 content-block">
            <h2>Search For Movie</h2>
            <form id="search-form">
                <input id="title-input" placeholder="Title"></input>
                <button id="search-button">Search</button>
            </form>
            <h3 id="search-title">Search Results</h3>
            <div id="search-list" class="row">
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-12 content-block">
            <h4>View Parties</h4>
            <div class="row">
                <div class="col-12" id="party-list">
                    {{#each parties}}
                        {{>party}}
                    {{/each}}
                </div>
            </div>
        </div>
    </div>

</div>
</div>
</div>

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

